{
  "name": "BitClout Plus",
  "description": "bitclout.com on steroids",
  "version": "0.1",
  "manifest_version": 3,
  "omnibox": { "keyword" : "bc" },
  "content_scripts": [
    {
      "matches": ["https://*.bitclout.com/*"],
      "js": ["main.js"],
      "css": ["main.css"],
      "run_at": "document_end"
    }
  ],
  "background": {
    "service_worker": "background.js"
  }
}